package com.day22.collection;

public class GenericArrayTest2 {

	public static void main(String[] args) {
		//구칙이 없는 경우 for문에서 초기화
		Array<Integer>list = new ArrayList<Integer>(4);
		
		//저장
		for(int i=0;i<3;i++) {
			list.add((i+1)*2);
			
		}
		
		//읽어오기
		for(int i=0;i<list.size();i++) {
			int n = list.get(i);
			System.out.println(n);
		}
		
		
	}

}
